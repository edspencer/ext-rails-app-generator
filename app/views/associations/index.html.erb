<h1>Listing associations</h1>

<table>
  <tr>
  </tr>

<% for association in @associations %>
  <tr>
    <td><%= link_to 'Show', association %></td>
    <td><%= link_to 'Edit', edit_association_path(association) %></td>
    <td><%= link_to 'Destroy', association, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New association', new_association_path %>
